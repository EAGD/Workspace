#!groovy



def function(n1,n2){
    def int num1_ = parseInt(n1)
    def int num2_ = parseInt(n2)
    def suma=0;
    suma = num1_ + num2_;
    echo suma
}

pipeline {
    agent any
    
    stages{
        stage("Function"){
            steps{
                function("${num1}","${num2}")
            }
        }
    }
}
