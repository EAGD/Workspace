#!groovy

def int num1_ = "${num1}"
def int num2_ = "${num2}"

def function(n1,n2){
    def suma=0;
    suma = n1 + n2;
    echo suma
}

pipeline {
    agent any
    
    stages{
        stage("Function"){
            steps{
                function("${num1_}","${num2_}")
            }
        }
    }
}